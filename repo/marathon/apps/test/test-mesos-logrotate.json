{
  "id": "/test-logs",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": "while true; do dd if=/dev/urandom of=./test.txt bs=1048576 count=25; cat ./test.txt; sleep infinity; done",
  "container": {
    "portMappings": [],
    "type": "MESOS",
    "volumes": []
  },
  "cpus": 0.1,
  "disk": 0,
  "env": {
    "CONTAINER_LOGGER_LOGROTATE_MAX_STDERR_SIZE": "30MB",
    "CONTAINER_LOGGER_LOGROTATE_MAX_STDOUT_SIZE": "30MB"
  },
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 128,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "healthChecks": [],
  "fetch": [],
  "constraints": []
}
