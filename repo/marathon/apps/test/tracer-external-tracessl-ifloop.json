{
  "volumes": null,
  "id": "/test/tracer-external-tracessl-ifloop-test",
  "cmd": null,
  "args": null,
  "user": null,
  "env": null,
  "instances": 1,
  "cpus": 0.1,
  "mem": 65,
  "disk": 0,
  "gpus": 0,
  "executor": null,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ]
  ],
  "fetch": null,
  "storeUrls": null,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "jeremykuhnash/tracer:v9",
      "forcePullImage": true,
      "privileged": false,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp",
          "servicePort": 10100
        }
      ],
      "network": "BRIDGE"
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "gracePeriodSeconds": 10,
      "intervalSeconds": 2,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 10,
      "ignoreHttp1xx": false
    }
  ],
  "readinessChecks": null,
  "dependencies": null,
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_SSL_CERT": "/etc/ssl/cert.pem",
    "HAPROXY_MAP_HTTPS_FRONTEND_ACL": "  tcp-request inspect-delay 5s\n  use_backend %[ssl_fc_sni,lower,map(/marathon-lb/domain2backend.map)] if {{ req.ssl_hello_type 1 }}\n  use_backend %[req.hdr(host),lower,regsub(:.*$,,),map(/marathon-lb/domain2backend.map)]\n",
    "HAPROXY_0_VHOST": "tracer4.external.example.com"
  },
  "acceptedResourceRoles": null,
  "residency": null,
  "secrets": null,
  "taskKillGracePeriodSeconds": null,
  "portDefinitions": null,
  "requirePorts": false
}
